{
  "name": "bootstrap-sandbox",
  "version": "1.0.0",
  "description": "Bootstrap Templates",
  "main": "index.js",
  "scripts": {
    "lint": "stylelint \"scss/*.scss\"",
    "scss:watch": "sass --watch scss/style.scss:css/style.css",
    "css:compile": "sass scss/style.scss css/style.css",
    "css:compile:compress": "sass --style compressed --source-map --embed-sources scss/style.scss css/style.css",
    "css:prefix": "postcss css/style.css --replace --use autoprefixer --map ",
    "css:build": "npm-run-all css:compile:compress css:prefix"
  },
  "keywords": [
    "bootstrap"
  ],
  "browserslist": [
    "defaults"
  ],
  "author": "majadc",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^10.4.14",
    "bootstrap": "^5.3.0"
  },
  "devDependencies": {
    "eslint": "^8.45.0",
    "eslint-plugin-compat": "^4.1.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.27",
    "postcss-cli": "^10.1.0",
    "stylelint": "^15.10.2",
    "stylelint-config-standard-scss": "^10.0.0"
  }
}